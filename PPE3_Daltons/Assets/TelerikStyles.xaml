            </Grid>
            <Grid x:Name="VerticalSeparator" Margin="2" Visibility="Collapsed" Width="2">
                <Rectangle Fill="{StaticResource MenuItemSeparatorLeft}" HorizontalAlignment="Left" Width="1"/>
                <Rectangle Fill="{StaticResource MenuItemSeparatorRight}" HorizontalAlignment="Right" Width="1"/>
